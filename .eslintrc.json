{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "es6": true,
        "jasmine": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "space-before-function-paren":"off",
        "accessor-pairs":"off",
        "eol-last":"off",
        "operator-linebreak":"off",
        "no-unused-vars":"warn",
        "no-multiple-empty-lines":"off"
    },
    "plugins": ["jasmine"]
}
