{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "es6": true,
        "jasmine": true,
        "codeceptjs/codeceptjs": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "space-before-function-paren": "off",
        "accessor-pairs": "off",
        "no-prototype-builtins": "off",
        "eol-last": "off",
        "operator-linebreak": "off",
        "quote-props": "off"
    },
    "plugins": ["jasmine","codeceptjs"]
}
